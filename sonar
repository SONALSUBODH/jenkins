pipeline {
    stages {
        stage ('code-pull'){
            steps {
                git 'https://github.com/mayurmwagh/student-ui.git'
            }
        }
        
        stage ('build'){
            steps {
                sh 'mvn clean package'
            }
        }
        stage ('testing'){
            steps {
              waitForQualityGate abortPipeline: false, credentialsId: 'sonar-tokan'  
                  sh 'mvn sonar:sonar project key=studentapp'
        }

      }
   }
}
